{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production",
    "AI_PROVIDER": "mock",
    "STELLAR_NETWORK": "testnet",
    "STELLAR_HORIZON_URL": "https://horizon-testnet.stellar.org"
  }
}
